{
  "name": "@remnote/plugin-sdk",
  "version": "0.0.46",
  "description": "An SDK to develop RemNote plugins.",
  "main": "dist/index.js",
  "bin": {
    "remnote-plugin": "scripts/index.js"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "scripts/index.js"
  ],
  "keywords": [
    "remnote",
    "plugin",
    "sdk",
    "api"
  ],
  "scripts": {
    "build": "bash scripts/build.sh",
    "_build": "webpack --config webpack.config.production.js",
    "watch:js": "yarn run _build -- --watch",
    "build:types": "tsc -p ./ --emitDeclarationOnly",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:tsc": "tsc --noEmit",
    "watch": "tsc -watch -p ./",
    "docs": "typedoc src --out docs"
  },
  "author": "remnote.com",
  "license": "ISC",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "esbuild-loader": "^2.20.0",
    "eslint": "^8.8.0",
    "ts-loader": "^9.3.1",
    "typedoc": "^0.22.11",
    "typescript": "^5.4.5",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "resolutions": {
    "@types/react": "^17.0.47"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "enquirer": "^2.3.6",
    "mitt": "^3.0.0",
    "nanoid": "^3.3.1",
    "os": "^0.1.2",
    "type-fest": "^2.12.1",
    "underscore": "^1.13.2",
    "zod": "^3.22.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}